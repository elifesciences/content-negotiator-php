{
    "name": "elife/content-negotiator",
    "description": "eLife Sciences content negotiator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\ContentNegotiator\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\ContentNegotiator\\": "./test"
        }
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "symfony/http-foundation": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "willdurand/negotiation": "^2.0.2 || ^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^9.6",
        "pimple/pimple": "^3.0",
        "silexandfriends/silex": "^2.2 || ^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^4.4 || ^5.0",
        "symfony/css-selector": "^4.4 || ^5.0",
        "symfony/debug": "^4.4 || ^5.0"
    },
    "conflict": {
        "sebastian/comparator": "<1.2.3"
    },
    "suggest": {
        "silex/silex": "^2.0, to use ContentNegotiationProvider"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
