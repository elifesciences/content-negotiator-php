{
    "name": "elife/content-negotiator",
    "description": "eLife Sciences content negotiator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\ContentNegotiator\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\ContentNegotiator\\": "./test"
        }
    },
    "require": {
        "php": "^7.0",
        "symfony/http-foundation": "^2.8.52 || ^3.4.35 || ^4.4",
        "symfony/http-kernel": "^2.7 || ^3.0 || ^4.4",
        "willdurand/negotiation": "^2.0.2 || ^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.7 || ^6.0",
        "pimple/pimple": "^3.0",
        "silex/silex": "^2.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^2.7 || ^3.0 || ^4.4",
        "symfony/css-selector": "^2.7 || ^3.0 || ^4.4",
        "symfony/debug": "^2.8 || ^3.0 || ^4.4"
    },
    "conflict": {
        "sebastian/comparator": "<1.2.3"
    },
    "suggest": {
        "silex/silex": "^2.0, to use ContentNegotiationProvider"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
