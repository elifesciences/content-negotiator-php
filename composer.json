{
    "name": "elife/content-negotiator",
    "description": "eLife Sciences content negotiator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\ContentNegotiator\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\ContentNegotiator\\": "./test"
        }
    },
    "require": {
        "php": "^7.0",
        "symfony/http-foundation": "^2.7 || ^3.0",
        "symfony/http-kernel": "^2.7 || ^3.0",
        "willdurand/negotiation": "^2.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.7 || ^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
